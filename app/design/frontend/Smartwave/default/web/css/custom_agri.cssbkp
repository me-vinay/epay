.mainbanner-container{
  position: relative;
}
.banner-left, .banner-text-a, .banner-text-b{
  position: absolute;
   /* box-shadow: 0px 0px 34px 0px #296180;*/
    color: #296180;
    font-size: 30px;
    /*background: #d8d8d7;*/
    padding: 15px 12px;
    /*border-radius: 15px;*/
    text-align: center;
}
.banner-left{
  left: 510px;
    top: 28%;
}
.banner-text-a{
  left: 434px;
  bottom: 24%;
  z-index: 3;
}
.banner-text-b{
    left: 652px;
    bottom: 35%;
    z-index: 3;
}
.box .content{
  font-weight: 900;
}
.banner-left a:hover, .banner-text-a a:hover, .banner-text-b a:hover{
    color:  #296180;
    text-decoration: none;
}
.banner-left a, .banner-text-a a, .banner-text-b a, .banner-text-b{
  color:  #296180;
  font-weight: 900;
}
.banner-left, .banner-text-a, .banner-text-b{
  animation: fadein 5s;
}
@keyframes fadein {
  from {
    opacity: 0}
  to {
    opacity: 1}
}
@keyframes videobg{
  from {
    right: -100%}
  to {
    right: 0%}
}
.banner-left:before {

    position: absolute;
    top: 20px;
    background: 'homepage/graphics/clip-1298.png';
}
#shop-sell, #shop-buy, #shop-board, #shop-bg{
    position: absolute;
}
#shop-board{
  width: 300px;
  top: 15%;
  left: 426px;
  animation: slideboard 1s;
}
@keyframes slideboard {
  from {
    left: -100%}
  to {
    left: 426px}
}
#shop-bg{
  width: 500px;
  top: 1%;
  left: 0px;
  z-index: 1;
  animation: slidebg 1s;
}
@keyframes slidebg {
  from {
    left: -100%}
  to {
    left: 0%}
}
#shop-buy{
    width: 232px;
    top: 55%;
    left: 622px;
    z-index: 2;
    animation: slidebuy 1s;
}
@keyframes slidebuy {
  from {
    left: -100%}
  to {
    left: 622px}
}
#shop-sell{
    width: 232px;
    top: 66%;
    left: 400px;
    z-index: 2;
    animation: slidesell 1s;
}
@keyframes slidesell{
  from {
    left: -100%}
  to {
    left: 400px}
}
