}
.short-text h3{
  font-size: 2rem;
    letter-spacing: .25px;
    margin-bottom: 1.5rem;
    color: #00577d;
    font-family: Circular-Black,sans-serif;
    font-weight: bold;
}
.short-text .teaser-text{
  line-height: 1.8rem;
    margin-bottom: 2rem;
}
.text-button{
  color: #f05541;
    font-family: Circular-Book,sans-serif;
    border-radius: 50px;
    background-color: transparent;
    border: 0;
    border-radius: 0px;
    padding: 10px 20px 10px 0;
    margin: 0px 0;
    letter-spacing: 2px;
    -webkit-transition: .5s ease-out;
    transition: .5s ease-out;
    text-transform: uppercase;
    font-size: 14pt;
    font-weight: 700;
    text-decoration: none;
}
.text-button:after{
  content: ' \279D';
    line-height: -4rem;
    font-weight: bold;
    font-size: 1.5rem;
}
